{{#if details.search_results}}

  <span class="p-title">{{fa-icon "info" fixedWidth=true}} NSRL RDS Search Results</span>

  {{#each details.search_results as |result index|}}
      <h1 class="p-title">
        Result #{{inc index}}
      </h1>
      {{#if result.result}}
        <div>
            <span class="p-key">Result: </span>
            <span class="p-value"><b>{{result.result}}</b></span>
        </div>
      {{/if}}
      {{#if result.knownlevel}}
        <div>
            <span class="p-key">Known Level: </span>
            <span class="p-value"><b>{{result.knownlevel}}</b></span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_file_name}}
        <div>
            <span class="p-key">File Name: </span>
            <span class="p-value">{{result.details.hashdd_file_name}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_size}}
        <div>
            <span class="p-key">File Size: </span>
            <span class="p-value">{{result.details.hashdd_size}} bytes</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_product_manufacturer}}
        <div>
            <span class="p-key">Product Manufacturer: </span>
            <span class="p-value">{{result.details.hashdd_product_manufacturer}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_product_name}}
        <div>
            <span class="p-key">Product Name: </span>
            <span class="p-value">{{result.details.hashdd_product_name}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_product_version}}
        <div>
            <span class="p-key">Product Version: </span>
            <span class="p-value">{{result.details.hashdd_product_version}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_md5}}
        <div>
            <span class="p-key">MD5: </span>
            <span class="p-value">{{result.details.hashdd_md5}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_sha1}}
        <div>
            <span class="p-key">SHA-1: </span>
            <span class="p-value">{{result.details.hashdd_sha1}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_sha256}}
        <div>
            <span class="p-key">SHA-256: </span>
            <span class="p-value">{{result.details.hashdd_sha256}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_sha512}}
        <div>
            <span class="p-key">SHA-512: </span>
            <span class="p-value">{{result.details.hashdd_sha512}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_productapplicationtype}}
        <div>
            <span class="p-key">Product Application Type: </span>
            <span class="p-value">{{result.details.hashdd_productapplicationtype}}</span>
        </div>
      {{/if}}
      {{#if result.details.hashdd_productlanguage}}
        <div>
            <span class="p-key">Product Language: </span>
            <span class="p-value">{{result.details.hashdd_productlanguage}}</span>
        </div>
      {{/if}}
      <hr>
  {{/each}}

{{/if}}
